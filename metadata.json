{
  "name": "eyp-saltstack",
  "version": "0.2.0",
  "author": "Jordi Prats",
  "summary": "saltstack - minion, master, api, cloud, syndic and ssh setup and configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/SaltaIT/eyp-saltstack",
  "project_page": "https://github.com/SaltaIT/eyp-saltstack",
  "issues_url": "https://github.com/SaltaIT/eyp-saltstack/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 1.0.0 < 9.9.9"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.2.3 < 9.9.9"},
    {"name":"eyp/eyplib","version_requirement":">= 1.0.0 < 2.0.0"},
    {"name":"eyp/python","version_requirement":">= 0.1.7 < 0.2.0"},
    {"name":"eyp/apt","version_requirement":">= 0.1.6 < 0.2.0"}
